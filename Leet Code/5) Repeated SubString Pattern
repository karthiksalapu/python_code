class Solution(object):
    def repeatedSubstringPattern(self, s):
        """
        :type s: str
        :rtype: bool
        """

        n = len(s)
        for length in range(1, n // 2 + 1):
            if n % length == 0:  
                substr = s[:length]  
                if substr * (n // length) == s: 
                    return True
        return False
